{
	"env": {
		"mocha": true
	},
	"plugins": ["mocha"],
	"globals": {
		"a11ySuite": false,
		"assert": false,
		"chai": true,
		"expect": false,
		"fixture": false,
		"flush": false,
		"MockInteractions": false,
		"sinon": true,
		"WCT": false,
		"getDomType": false,
		"setupOmnitableFixture":false
	},
	"rules": {
		"no-unused-vars": [
			"warn",
			{"varsIgnorePattern":"omnitable"}
		],
		"no-unused-expressions": "off"
	}
}
